<script>
    var isKuxun = function () {
        var regex = new RegExp('[\\?&]train_source=([^&#]*)'),
                results = regex.exec(location.search);
        var hostStr = null ? '' : decodeURIComponent(results && results[1].replace(/\+/g, ' '));
        var kxApp = ['kx@iphone', 'kx@android', 'kx@android_5.6', 'kx@wap'];
        return hostStr && kxApp.some(function (item) {
                    return hostStr.toLowerCase().indexOf(item) > -1;
                });
    };
    var isOffline =  (function (){
        var platform = 'h5';
        var ua = navigator.userAgent || '';

        if (!isKuxun() && ua.match(/iHotel/i)) {
            if (ua.match(/iPhone|iPad/i)) {
                platform = 'ios';
            } else if (ua.match(/Android/i)) {
                platform = 'android';
            }
        }

        return platform !== 'h5';
    })()
</script>

